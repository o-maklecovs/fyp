<main class="profile">
    <section id="aside-toggler">
        <div>Profile</div>
        <div class="arrow"></div>
    </section>
    <aside>
        {{#each links}}
            <a href="{{this.link}}">{{this.text}}</a>
        {{/each}}
    </aside>
    <section class="profile-jobs-list">
        <h2>Applied jobs</h2>
        <div class="item">
            <div>
                <a href="/job">Software Developer</a><br>
                <span class="company">Google</span>
            </div>
            <span>London</span>
            <span>03/01/2022</span>
        </div>
        <div class="item">
            <div>
                <a href="/job">Warehouse Worker</a><br>
                <span class="company">Amazon</span>
            </div>
            <span>Manchester</span>
            <span>11/01/2022</span>
        </div>
    </section>
</main>
<script src="javascript/aside.js"></script>